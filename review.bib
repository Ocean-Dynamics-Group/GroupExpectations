@article{stone_baroclinic_1971,
	title = {Baroclinic stability under non-hydrostatic conditions},
	volume = {45},
	issn = {1469-7645, 0022-1120},
	url = {https://www.cambridge.org/core/journals/journal-of-fluid-mechanics/article/div-classtitlebaroclinic-stability-under-non-hydrostatic-conditionsdiv/B524523F6D3F876A378E6C9986A8C32C},
	doi = {10.1017/S0022112071000260},
	abstract = {Eady's model for the stability of a thermal wind in an inviscid, stratified, rotating system is modified to allow for deviations from hydrostatic equilibrium. The stability properties of the flow are uniquely determined by two parameters: the Richardson number Ri, and the ratio of the aspect ratio to the Rossby number $\delta$. The latter parameter may be taken as a measure of the deviations from hydrostatic equilibrium ($\delta$ = 0 in Eady's model). It is found that such deviations decrease the growth rates of all three kinds of instability which can occur in this problem: {\textquoteleft}geostrophic{\textquoteright} baroclinic instability, symmetric instability, and Kelvin{\textendash}Helmholtz instability. The unstable wavelengths for {\textquoteleft}geostrophic{\textquoteright} and Kelvin{\textendash}Helmholtz instability are increased for finite values of $\delta$, while the unstable wavelengths for symmetric instability are unaffected. The {\textquoteleft}non-hydrostatic{\textquoteright} effects ($\delta$ /= 0) are significant for symmetric and Kelvin{\textendash}Helmholtz instability when $\delta$ [gsim ] 1, but not for {\textquoteleft}geostrophic{\textquoteright} instability unless $\delta$ [Gt ] 1. Consequently, the first two types of instability tend to be suppressed relative to {\textquoteleft}geostrophic{\textquoteright} instability by {\textquoteleft}non-hydrostatic{\textquoteright} conditions. Figure 3 summarizes the different instability r{\'e}gimes that can occur. In laboratory experiments symmetric instability can be studied best when $\delta$ [lsim ] 1, while Kelvin{\textendash}Helmholtz instability can be studied best when $\delta$ [Lt ] 1.},
	number = {4},
	urldate = {2017-12-03},
	journal = {Journal of Fluid Mechanics},
	author = {Stone, Peter H.},
	month = feb,
	year = {1971},
	pages = {659--671}
}

