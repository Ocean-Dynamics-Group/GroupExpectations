@article{naveira_garabato_rapid_2019,
	title = {Rapid mixing and exchange of deep-ocean waters in an abyssal boundary current},
	volume = {116},
	issn = {0027-8424, 1091-6490},
	url = {http://www.pnas.org/lookup/doi/10.1073/pnas.1904087116},
	doi = {10.1073/pnas.1904087116},
	abstract = {The overturning circulation of the global ocean is critically shaped by deep-ocean mixing, which transforms cold waters sinking at high latitudes into warmer, shallower waters. The effectiveness of mixing in driving this transformation is jointly set by two factors: the intensity of turbulence near topography and the rate at which well-mixed boundary waters are exchanged with the stratified ocean interior. Here, we use innovative observations of a major branch of the overturning circulation{\textemdash}an abyssal boundary current in the Southern Ocean{\textemdash}to identify a previously undocumented mixing mechanism, by which deep-ocean waters are efficiently laundered through intensified near-boundary turbulence and boundary{\textendash}interior exchange. The linchpin of the mechanism is the generation of submesoscale dynamical instabilities by the flow of deep-ocean waters along a steep topographic boundary. As the conditions conducive to this mode of mixing are common to many abyssal boundary currents, our findings highlight an imperative for its representation in models of oceanic overturning.},
	language = {en},
	number = {27},
	urldate = {2019-07-17},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Naveira Garabato, Alberto C. and Frajka-Williams, Eleanor E. and Spingys, Carl P. and Legg, Sonya and Polzin, Kurt L. and Forryan, Alexander and Abrahamsen, E. Povl and Buckingham, Christian E. and Griffies, Stephen M. and McPhail, Stephen D. and Nicholls, Keith W. and Thomas, Leif N. and Meredith, Michael P.},
	month = jul,
	year = {2019},
	pages = {13233--13238},
	file = {Full Text:/Users/jwenegrat/Zotero/storage/CIG3DSS3/Naveira Garabato et al. - 2019 - Rapid mixing and exchange of deep-ocean waters in .pdf:application/pdf}
}

