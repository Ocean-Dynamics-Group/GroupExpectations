@article{mcwilliams_filament_2015,
	title = {Filament {Frontogenesis} by {Boundary} {Layer} {Turbulence}},
	issn = {0022-3670},
	url = {http://journals.ametsoc.org/doi/abs/10.1175/JPO-D-14-0211.1},
	doi = {10.1175/JPO-D-14-0211.1},
	abstract = {AbstractA submesoscale filament of dense water in the oceanic surface layer can undergo frontogenesis with a secondary circulation that has a surface horizontal convergence and downwelling in its center. This occurs either due to mesoscale straining deformation or due to the surface boundary layer turbulence that causes vertical eddy momentum flux divergence or, more briefly, vertical momentum mixing. In the latter case the circulation approximately has a linear horizontal momentum balance among the baroclinic pressure gradient, Coriolis force, and vertical momentum mixing, i.e., a turbulent thermal wind. The frontogenetic evolution induced by the turbulent mixing sharpens the transverse gradient of the longitudinal velocity (i.e., it increases the vertical vorticity) through convergent advection by the secondary circulation. In an approximate model based on the turbulent thermal wind, the central vorticity approaches a finite-time singularity, and in a more general hydrostatic model the central vorticity and horizontal convergence are amplified by shrinking the transverse scale to near the model{\textquoteright}s resolution limit within a short advectve period on the order of a day.},
	urldate = {2015-08-03},
	journal = {Journal of Physical Oceanography},
	author = {McWilliams, James C. and Gula, Jonathan and Molemaker, M. Jeroen and Renault, Lionel and Shchepetkin, Alexander F.},
	month = may,
	year = {2015}
}

